{
    "name": "vicephp/virtue",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Virtue\\Access\\": "vicephp/Virtue-Access/src/Access/",
            "Virtue\\Api\\": "vicephp/Virtue-Api/src/Api/",
            "Virtue\\DataTables\\": "vicephp/Virtue-DataTables/src/DataTables/",
            "Virtue\\Database\\": "vicephp/Virtue-PDO/src/Database/",
            "Virtue\\Forms\\": "vicephp/Virtue-Forms/src/Forms/",
            "Virtue\\Http\\": "vicephp/Virtue-Http/src/Http/",
            "Virtue\\JWT\\": "vicephp/Virtue-JWT/src/JWT/",
            "Virtue\\View\\": "vicephp/Virtue-View/src/View/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Virtue\\Access\\": "vicephp/Virtue-Access/tests/Access/",
            "Virtue\\Api\\": "vicephp/Virtue-Api/tests/Api/",
            "Virtue\\DataTables\\": "vicephp/Virtue-DataTables/tests/DataTables/",
            "Virtue\\Database\\": "vicephp/Virtue-PDO/tests/Database/",
            "Virtue\\Forms\\": "vicephp/Virtue-Forms/tests/Forms/",
            "Virtue\\Http\\": "vicephp/Virtue-Http/tests/Http/",
            "Virtue\\JWT\\": "vicephp/Virtue-JWT/tests/JWT/",
            "Virtue\\View\\": "vicephp/Virtue-View/tests/View/"
        }
    },
    "replace": {
        "vicephp/virtue-access": "self.version",
        "vicephp/virtue-api": "self.version",
        "vicephp/virtue-datatables": "self.version",
        "vicephp/virtue-forms": "self.version",
        "vicephp/virtue-http": "self.version",
        "vicephp/virtue-jwt": "self.version",
        "vicephp/virtue-pdo": "self.version",
        "vicephp/virtue-view": "self.version"
    },
    "description": "Making PHP more Pythonic...",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "guzzlehttp/psr7": ">=1.6",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "slim/php-view": "^2.0",
        "slim/slim": "~4.0",
        "webmozart/assert": "^1.0"
    },
    "require-dev": {
        "ext-SimpleXML": "*",
        "ext-dom": "*",
        "http-interop/http-factory-guzzle": "^1.0",
        "mockery/mockery": "^1.2",
        "php-di/php-di": "^6.0",
        "phpstan/phpstan": "^0.12.0",
        "phpunit/phpunit": "^8.0 | ^9.0",
        "psr/log": "^1.1",
        "symplify/monorepo-builder": "^8.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
