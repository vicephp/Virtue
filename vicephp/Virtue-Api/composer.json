{
    "name": "virtue/api",
    "type": "library",
    "description": "Making PHP more Pythonic",
    "keywords": ["framework","micro","api","router"],
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "slim/psr7": "dev-master",
        "slim/slim": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.2",
        "php-di/php-di": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "Virtue\\Api\\": "src/Api/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Virtue\\Api\\": "tests/Api/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
