{
  "name": "virtue/access",
  "type": "library",
  "description": "Making PHP more Pythonic",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  },
  "require": {
    "php": "^7.1",
    "beberlei/assert": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Virtue\\Access\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Virtue\\Access\\": ["tests/"]
    }
  }
}
