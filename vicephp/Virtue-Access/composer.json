{
    "name": "vicephp/virtue-access",
    "type": "library",
    "description": "Making access management more Pythonic...",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": "^7.1",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "webmozart/assert": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpstan/phpstan": "^0.12.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Virtue\\Access\\": "src/Access/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Virtue\\Access\\": "tests/Access/"
        }
    }
}
