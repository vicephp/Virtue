{
  "name": "vicephp/virtue-pdo",
  "type": "library",
  "description": "Making PDO more Pythonic...",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require": {
    "php": "^7.1",
    "ext-pdo": "*",
    "webmozart/assert": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.2",
    "phpstan/phpstan": "^0.12.0"
  },
  "autoload": {
    "psr-4": {
      "Virtue\\Database\\": "src/Database/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Virtue\\Database\\": "tests/Database/"
    }
  }
}
