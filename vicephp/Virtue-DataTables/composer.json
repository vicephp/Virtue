{
    "name": "vicephp/virtue-datatables",
    "type": "library",
    "description": "Making work with jQuery DataTables more Pythonic...",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 | ^9.0",
        "phpstan/phpstan": "^0.12.0"
    },
    "autoload": {
        "psr-4": {
            "Virtue\\DataTables\\": "src/DataTables/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Virtue\\DataTables\\": "tests/DataTables/"
        }
    }
}
