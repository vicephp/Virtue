{
  "name": "virtue/datatables",
  "type": "library",
  "description": "I'm gonna go build my own datatables lib, with black jack and hookers!",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  },
  "require": {
    "php": "^7.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Virtue\\DataTables\\": "src/DataTables"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Virtue\\DataTables\\": ["tests/unit/DataTables"]
    }
  }
}
