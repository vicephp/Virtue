{
    "name": "vicephp/virtue-session",
    "type": "library",
    "description": "Making session handling more Pythonic...",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Virtue\\Session\\": "src/Session/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Virtue\\Session\\": "tests/Session/"
        }
    }
}
