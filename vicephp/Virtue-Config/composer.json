{
    "name": "vicephp/virtue-config",
    "type": "library",
    "description": "Making Config more Pythonic...",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.0",
        "phpunit/phpunit": "^8.0 | ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Virtue\\Config\\": "src/Config/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Virtue\\Config\\": "tests/Config/"
        }
    }
}
